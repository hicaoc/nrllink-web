        "chalk": "5.6.2",
        "eslint": "^9.0.0",
        "eslint-plugin-vue": "^10.7.0",
        "fast-glob": "^3.3.3",
        "husky": "9.1.7",
        "jsdom": "^28.0.0",
        "lint-staged": "16.2.7",
        "plop": "4.0.5",
        "postcss": "^8.5.6",
        "runjs": "^4.4.2",
        "sass": "^1.97.3",
        "svgo": "4.0.0",
