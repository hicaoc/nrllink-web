        "screenfull": "6.0.2",
        "showdown": "2.1.0",
        "sortablejs": "1.15.6",
        "uni-webview-lib": "^1.5.2",
        "update-browserslist-db": "^1.2.3",
        "vue": "^3.5.28",
        "vue-i18n": "^11.2.8",
        "vue-router": "^5.0.2",
        "vue3-count-to": "^1.1.2",
