        "@vue/cli-plugin-unit-jest": "4.5.18",
        "@vue/cli-service": "4.5.18",
        "@vue/test-utils": "2.2.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.0.3",
        "babel-jest": "24.9.0",
        "chalk": "3.0.0",
        "chokidar": "3.3.0",
        "connect": "3.7.0",
        "eslint": "7.6.0",
        "eslint-plugin-vue": "6.2.2",
        "html-webpack-plugin": "4.3.0",
        "husky": "3.1.0",
        "lint-staged": "9.4.3",
        "mockjs": "1.1.0",
