{
    "name": "sandong-manager",
    "version": "4.0.1",
    "description": "HAM互联",
    "author": "CaoCheng <cao@live.com>",
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build:prod": "vite build",
        "build:stage": "vite build --mode staging",
        "preview": "vite preview",
        "lint": "eslint --ext .js,.vue src",
        "test:unit": "vitest run",
        "test:ci": "npm run lint && npm run test:unit",
        "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
        "new": "plop"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,vue}": [
            "eslint --fix",
            "git add"
        ]
    },
    "keywords": [
        "vue",
        "admin",
        "dashboard",
        "element-ui",
        "boilerplate",
        "admin-template",
        "management-system"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PanJiaChen/vue-element-admin.git"
    },
    "bugs": {
        "url": "https://github.com/PanJiaChen/vue-element-admin/issues"
    },
    "dependencies": {
        "axios": "^0.21.4",
        "clipboard": "2.0.11",
        "codemirror": "5.65.13",
        "core-js": "^3.30.1",
        "driver.js": "0.9.8",
        "dropzone": "5.9.3",
        "echarts": "4.9.0",
        "element-plus": "^2.7.6",
        "file-saver": "2.0.5",
        "fuse.js": "3.6.1",
        "js-cookie": "2.2.1",
        "jshint": "^2.13.6",
        "jsonlint": "1.6.3",
        "jszip": "3.10.1",
        "normalize.css": "8.0.1",
        "nprogress": "0.2.0",
        "path-to-regexp": "^6.3.0",
        "pinia": "^2.1.7",
        "rimraf": "^5.0.0",
        "screenfull": "5.2.0",
        "showdown": "1.9.1",
        "sortablejs": "1.15.0",
        "tasksfile": "^5.1.1",
        "uni-webview-lib": "^1.5.2",
        "update-browserslist-db": "^1.0.11",
        "vue": "^3.4.31",
        "vue-i18n": "^9.9.1",
        "vue-router": "^4.4.0",
        "vue3-count-to": "^1.1.1",
