        "autoprefixer": "^10.4.19",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "24.9.0",
        "chalk": "3.0.0",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.26.0",
        "husky": "3.1.0",
        "jsdom": "^22.1.0",
        "lint-staged": "9.5.0",
        "plop": "2.7.6",
        "postcss": "^8.4.38",
        "runjs": "^4.4.2",
        "sass": "^1.77.6",
        "svgo": "1.3.2",
        "unplugin-auto-import": "^0.17.6",
        "unplugin-vue-components": "^0.27.0",
        "vite": "^4.5.3",
